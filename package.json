{
  "name": "pool-maintainer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5080",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit --skipLibCheck",
    "quality": "bun run lint && bun run format:check && bun run type-check",
    "quality:fix": "bun run lint:fix && bun run format && bun run type-check",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:contrast": "node scripts/contrast-test.js",
    "test:contrast:json": "node scripts/contrast-test.js --report=json",
    "test:contrast:aaa": "node scripts/contrast-test.js --standard=AAA",
    "test:all": "bun run test:run && bun run test:e2e && bun run test:visual && bun run test:contrast",
    "storybook": "storybook dev -p 6080 --host 0.0.0.0",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook --coverage",
    "validate": "bun run quality && bun run test:run && bun run test:contrast && bun run build",
    "validate:full": "bun run quality && bun run test:all && bun run build && bun run build-storybook",
    "clean": "rm -rf dist build coverage .nyc_output storybook-static",
    "reset": "bun run clean && rm -rf node_modules && bun install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.532.0",
    "react": "^19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-actions": "8.6.14",
    "@storybook/addon-controls": "8.6.14",
    "@storybook/addon-docs": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "color-contrast-checker": "^2.1.0",
    "culori": "^4.0.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.18",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "postcss-value-parser": "^4.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "puppeteer": "^24.15.0",
    "storybook": "8.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "wcag-contrast": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "bash -c 'bunx tsc --noEmit --skipLibCheck'"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.stories.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/test/fixtures/**/*.ts": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "**/CLAUDE.md": [
      "prettier --write --parser markdown"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
